<h1>Currency Rankings</h1>
<br/><br/>

<% @coins %>
    <% for x in @coins %>
    <% for coin in @my_coins %>
        <% if coin == x['rank'] %>
        <strong><%= x['rank'] %>)</strong>
            <%= x['name'] %><br>
            <%= number_to_currency(x['price_usd']) %>
            <br><br>
        <% end %>
    <% end %>
   <% end %>
